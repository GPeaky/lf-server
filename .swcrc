{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },

        "target": "es2020",
        "paths": {
            "@config/*": ["development/config/*"]
        },

        "baseUrl": "."
    },

    "minify": true,
    "exclude": "tests/",
    "module": {
        "type": "commonjs"
    }
}