{
    "compilerOptions": {
        "allowJs": false,
        "module": "ESNext",
        "target": "ES2022",
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowUnreachableCode": false,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noUncheckedIndexedAccess": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}
