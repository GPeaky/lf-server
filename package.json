    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
